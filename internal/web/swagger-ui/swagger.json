                        " 'user']"
                    ]
                },
                "status": {
                    "description": "User status",
                    "type": "string",
                    "example": "active"
                },
                "tenant": {
                    "description": "Organization Context",
                    "type": "string",
                    "example": "acme"
                },
                "trusted_devices": {
                    "description": "Devices",
                    "type": "string",
                    "example": "device1,device2"
                },
                "updated_at": {
                    "type": "string",
                    "example": "2024-01-01T00:00:00Z"
                },
                "username": {
                    "type": "string",
                    "example": "john.doe"
                },
                "webauthn_locked": {
                    "type": "boolean",
                    "example": false
