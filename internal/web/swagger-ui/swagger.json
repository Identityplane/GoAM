                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Realm ID",
                        "name": "realm",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Flow path/name",
                        "name": "path",
