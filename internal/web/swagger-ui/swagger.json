        "/{tenant}/{realm}/auth/{path}": {
            "get": {
                "description": "Handles authentication requests by executing the specified flow. Returns either a prompt for user input or a final result. Supports debug mode for additional information.",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "text/html"
                ],
                "tags": [
                    "Authentication"
                ],
                "summary": "Process authentication request",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Tenant ID",
                        "name": "tenant",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Realm ID",
                        "name": "realm",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Flow path/name",
                        "name": "path",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "boolean",
                        "description": "Enable debug mode",
                        "name": "debug",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Current step in the flow",
                        "name": "step",
                        "in": "formData"
                    },
                    {
                        "type": "string",
                        "description": "User input for the current step's prompts",
                        "name": "{prompt_key}",
                        "in": "formData"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "HTML response containing either a prompt form or the final result",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "Realm or flow not found",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "post": {
                "description": "Handles authentication requests by executing the specified flow. Returns either a prompt for user input or a final result. Supports debug mode for additional information.",
                "consumes": [
